{
    "name": "ajz/anthropic",
    "description": "Laravel package for Anthropic AI API integration",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "*",
        "illuminate/http": "*",
        "pusher/pusher-php-server": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^9.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ajz\\Anthropic\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Murtadah Haddad",
            "email": "murtadah.haddad@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Ajz\\Anthropic\\AnthropicServiceProvider"
            ],
            "aliases": {
                "Anthropic": "Ajz\\Anthropic\\Facades\\Anthropic"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

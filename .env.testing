# Testing Environment Configuration

# Anthropic API Settings
ANTHROPIC_API_KEY=test-api-key-123
ANTHROPIC_API_URL=https://api.anthropic.com/v1
ANTHROPIC_API_VERSION=2024-01-01
ANTHROPIC_API_TIMEOUT=30
ANTHROPIC_API_RETRY_TIMES=3
ANTHROPIC_API_RETRY_SLEEP=100

# Rate Limiting Settings
ANTHROPIC_RATE_LIMITING_ENABLED=true
ANTHROPIC_RATE_LIMIT_MAX_REQUESTS=60
ANTHROPIC_RATE_LIMIT_DECAY_MINUTES=1
ANTHROPIC_RATE_LIMIT_CACHE_DRIVER=array

# Cache Settings
ANTHROPIC_CACHE_ENABLED=true
ANTHROPIC_CACHE_TTL=3600
ANTHROPIC_CACHE_PREFIX=anthropic:test:
ANTHROPIC_CACHE_STORE=array
ANTHROPIC_CACHE_TAGS_ENABLED=true

# Default Model Settings
ANTHROPIC_DEFAULT_MODEL=claude-3-5-sonnet-20241022
ANTHROPIC_MAX_TOKENS=1024
ANTHROPIC_TEMPERATURE=0.7
ANTHROPIC_TOP_P=1.0
ANTHROPIC_TIMEOUT=60

# Logging Settings
ANTHROPIC_LOGGING_ENABLED=true
ANTHROPIC_LOG_CHANNEL=stack
ANTHROPIC_LOG_LEVEL=debug
ANTHROPIC_LOG_SEPARATE_FILES=false

# Agent-Specific Settings
ANTHROPIC_DEVELOPER_MODEL=claude-3-5-sonnet-20241022
ANTHROPIC_DEVELOPER_MAX_TOKENS=2048
ANTHROPIC_ARCHITECT_MODEL=claude-3-5-sonnet-20241022
ANTHROPIC_ARCHITECT_MAX_TOKENS=4096
ANTHROPIC_SECURITY_MODEL=claude-3-5-sonnet-20241022
ANTHROPIC_SECURITY_MAX_TOKENS=2048

# Team Settings
ANTHROPIC_TEAM_MAX_ROUNDS=5

# Validation Settings
ANTHROPIC_MAX_PROMPT_LENGTH=4000
ANTHROPIC_MAX_CONTEXT_LENGTH=8000
ANTHROPIC_MAX_FILE_SIZE=1048576

# Laravel Settings
APP_ENV=testing
APP_KEY=base64:your-testing-key-here
APP_DEBUG=true
APP_URL=http://localhost

# Database Settings
DB_CONNECTION=sqlite
DB_DATABASE=:memory:

# Cache & Queue Settings
CACHE_DRIVER=array
QUEUE_CONNECTION=sync
SESSION_DRIVER=array
